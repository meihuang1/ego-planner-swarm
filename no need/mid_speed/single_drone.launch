<launch>    
    <!-- 定义参数，从配置文件读取 -->
    <arg name="origin_lat" value="30.0"/>
    <arg name="origin_lon" value="120.0"/>
    <arg name="origin_alt" value="0.0"/>

    <arg name="map_size_x"/>
    <arg name="map_size_y"/>
    <arg name="map_size_z"/>
    <arg name="odom_topic" value="visual_slam/odom"/>

    <arg name="drone_id"/>
    <arg name="target_x"/>
